<nav class="mt-5" aria-label="Page navigation">
  <ul class="pagination justify-content-center">
    <li class="page-item <%= !pageData.previous && 'disabled' %>">
      <a class="page-link" href="<%= pageData.queryString %>page=<%= pageData.page - 1 %>">Previous</a>
    </li>

    <% pageData.pages.forEach(page => { %> 

        <li id="<%= page %> " class="page-item <%= pageData.page == page && 'active' %>">
            <a class="page-link" href="<%= pageData.queryString %>page=<%= page %>"><%= page %> </a>
        </li>

    <% }) %> 
    <!-- <li class="page-item"><a class="page-link" href="#">1</a></li>
    <li class="page-item"><a class="page-link" href="#">2</a></li>
    <li class="page-item"><a class="page-link" href="#">3</a></li> -->
    <li class="page-item <%= !pageData.next && 'disabled' %>">
      <a class="page-link" href="<%= pageData.queryString %>page=<%= pageData.page + 1 %>">Next</a>
    </li>
  </ul>
</nav>