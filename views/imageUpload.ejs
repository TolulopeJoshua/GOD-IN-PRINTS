<% layout('layouts/boilerplate') %> 
<div class="row my-5">
    <h1 class="text-center"><%= msg %> </h1>
    <div class="col-sm-8 offset-sm-2 col-md-6 offset-md-3 px-5">
        <form action="<%= route %> " method="POST" novalidate class="validated-form" enctype="multipart/form-data">
            <div class="my-3">
                <label class="form-label" for="image">Image:</label>
                <input class="form-control" type="file" id="image" name="image" required>
                <div class="valid-feedback">
                    Looks good!
                  </div>
            </div>
            <div class="my-5 text-center">                
                <button class="btn btn-success">Add Image</button>
            </div>
        </form>
    </div>
</div>

<script>
    const pageTitle = <%- JSON.stringify(route.split('/')[1]) %>;
</script>